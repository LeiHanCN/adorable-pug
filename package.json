{
  "name": "adorable-pug",
  "version": "0.0.0",
  "description": "adorable-pug",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "yarn pre && run-p dev-*",
    "build": "yarn pre && run-p build-*",
    "pre": "rm -rf dist/ && yarn cp-assets",
    "cp-assets": "mkdirp dist/ && cp -frp src/assets dist/",
    "dev-html": "yarn build-html --watch",
    "dev-js": "yarn build-js --watch",
    "build-html": "pug src/**/*.pug --out dist/ --pretty",
    "build-js": "babel src/**/*.js --out-dir dist/"
  },
  "author": "leihancn",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6"
  }
}
